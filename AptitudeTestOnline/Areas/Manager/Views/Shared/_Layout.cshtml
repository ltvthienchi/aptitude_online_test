<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>

    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

    <link rel="stylesheet" href="~/Assets/default/vendors/iconfonts/mdi/css/materialdesignicons.min.css">
    <link rel="stylesheet" href="~/Assets/default/vendors/css/vendor.bundle.base.css">
    <link rel="stylesheet" href="~/Assets/default/vendors/css/vendor.bundle.addons.css">
    <link rel="stylesheet" href="~/Assets/default/css/style.css">
    <link href="~/Assets/myStyle.css" rel="stylesheet" />
    <link rel="shortcut icon" href="~/Assets/default/images/favicon.png" />

</head>
<body>

    <div class="container-scroller">

        <nav class="navbar default-layout col-lg-12 col-12 p-0 fixed-top d-flex flex-row">
            <div class="text-center navbar-brand-wrapper d-flex align-items-top justify-content-center">
                <a class="navbar-brand brand-logo" href="index.html">

                </a>
                <a class="navbar-brand brand-logo-mini" href="index.html">

                </a>
            </div>
            <div class="navbar-menu-wrapper d-flex align-items-center">
                <ul class="navbar-nav navbar-nav-left header-links d-none d-md-flex">
                    <li class="nav-item">
                        @Html.ActionLink("Home", "Index", "Home")
                    </li>
                    <li class="nav-item">
                        @Html.ActionLink("About", "About", "Home")
                    </li>
                    <li class="nav-item">
                        @Html.ActionLink("Contact", "Contact", "Home")
                    </li>
                    <li class="nav-item">
                        @Html.ActionLink("Manager", "Index", "Manager/Home")
                    </li>
                </ul>
                <ul class="navbar-nav navbar-nav-right">
                    @Html.Partial("_LoginPartial")
                </ul>
                <button class="navbar-toggler navbar-toggler-right d-lg-none align-self-center" type="button" data-toggle="offcanvas">
                    <span class="mdi mdi-menu"></span>
                </button>
            </div>
        </nav>


        <!-- partial -->
        <div class="container-fluid page-body-wrapper">

            <nav class="sidebar sidebar-offcanvas" id="sidebar-client">
                <ul class="nav">
                    <li class="nav-item nav-profile">
                        <div class="nav-link">
                            <div class="user-wrapper">
                                <div class="profile-image">

                                </div>
                                <div class="text-wrapper">
                                    <p class="profile-name">Richard V.Welsh</p>
                                    <div>
                                        <small class="designation text-muted">Manager</small>
                                        <span class="status-indicator online"></span>
                                    </div>
                                </div>
                            </div>
                            <button class="btn btn-success btn-block">
                                New Project
                                <i class="mdi mdi-plus"></i>
                            </button>
                        </div>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="index.html">
                            <i class="menu-icon mdi mdi-television"></i>
                            <span class="menu-title">Dashboard</span>
                        </a>
                    </li>
                    <li class="dropdown-divider"></li>
                </ul>
            </nav>

            <div class="main-panel">

                @RenderBody()

                <footer class="footer">
                    <div class="container-fluid clearfix">
                        <span class="text-muted d-block text-center text-sm-left d-sm-inline-block">
                            Copyright © 2018
                            <a href="~/Assets/default/http://www.bootstrapdash.com/" target="_blank">Bootstrapdash</a>. All rights reserved.
                        </span>
                        <span class="float-none float-sm-right d-block mt-1 mt-sm-0 text-center">
                            Hand-crafted & made with
                            <i class="mdi mdi-heart text-danger"></i>
                        </span>
                    </div>
                </footer>
                <!-- partial -->
            </div>
            <!-- main-panel ends -->
        </div>
        <!-- page-body-wrapper ends -->
    </div>

    <script src="~/Assets/default/vendors/js/vendor.bundle.base.js"></script>
    <script src="~/Assets/default/vendors/js/vendor.bundle.addons.js"></script>
    <script src="~/Assets/default/js/off-canvas.js"></script>
    <script src="~/Assets/default/js/misc.js"></script>
    <script src="~/Assets/default/js/dashboard.js"></script>



    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
