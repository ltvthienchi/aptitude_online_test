<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>

    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

    <link rel="shortcut icon" href="~/Assets/default/images/favicon.png" />
    <link rel="stylesheet" href="~/Assets/default/vendors/iconfonts/mdi/css/materialdesignicons.min.css">
    <link rel="stylesheet" href="~/Assets/default/vendors/css/vendor.bundle.base.css">
    <link rel="stylesheet" href="~/Assets/default/vendors/css/vendor.bundle.addons.css">
    <link rel="stylesheet" href="~/Assets/default/css/style.css">
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.4.1/css/all.css">
    <link rel="stylesheet" href="~/Content/Site.css" />

</head>
<body>

    <div class="container-scroller">

        <nav class="navbar default-layout col-lg-12 col-12 p-0 fixed-top d-flex flex-row">
            <div class="text-center navbar-brand-wrapper d-flex align-items-top justify-content-center">
                <a class="navbar-brand brand-logo" href="/" style="height: 100%">
                    <img src="~/Assets/default/images/logo-oil.png" style="height: 100%; width: 60px" />
                </a>
                <a class="navbar-brand brand-logo-mini" href="index.html">

                </a>
            </div>
            <div class="navbar-menu-wrapper d-flex align-items-center">
                <ul class="navbar-nav navbar-nav-right">
                    @Html.Partial("_LoginPartial")
                </ul>
                <button class="navbar-toggler navbar-toggler-right d-lg-none align-self-center" type="button" data-toggle="offcanvas">
                    <span class="mdi mdi-menu"></span>
                </button>
            </div>
        </nav>


        <!-- partial -->
        <div class="container-fluid page-body-wrapper">

            <nav class="sidebar sidebar-offcanvas" id="sidebar-client">
                <ul class="nav">

                    <li class="nav-item">
                        <div class="nav-link" style="margin-top: 10px">
                            <div class="user-wrapper">
                                <div class="profile-image">

                                </div>
                                <div class="text-wrapper">
                                    @Html.ActionLink("Candidate", "Index", "Candidate")
                                </div>
                            </div>
                        </div>
                    </li>
                    <li class="dropdown-divider"></li>
                    <!---->
                    <li class="nav-item">
                        <div class="nav-link" style="margin-top: 10px">
                            <div class="user-wrapper">
                                <div class="profile-image">

                                </div>
                                <div class="text-wrapper">
                                    @Html.ActionLink("Result", "Index", "Users")
                                </div>
                            </div>
                        </div>
                    </li>
                    <li class="dropdown-divider"></li>
                    <!---->
                    <li class="nav-item">
                        <div class="nav-link" style="margin-top: 10px">
                            <div class="user-wrapper">
                                <div class="profile-image">

                                </div>
                                <div class="text-wrapper">
                                    @Html.ActionLink("Type Of Questions", "Index", "TypeOfQuestions")
                                </div>
                            </div>
                        </div>
                    </li>
                    <li class="dropdown-divider"></li>
                    <!---->
                    <li class="nav-item">
                        <div class="nav-link" style="margin-top: 10px">
                            <div class="user-wrapper">
                                <div class="profile-image">

                                </div>
                                <div class="text-wrapper">
                                    @Html.ActionLink("Questions", "Index", "Questions")
                                </div>
                            </div>
                        </div>
                    </li>
                    <li class="dropdown-divider"></li>
                    <!---->

                    <li class="nav-item">
                        <div class="nav-link" style="margin-top: 10px">
                            <div class="user-wrapper">
                                <div class="profile-image">

                                </div>
                                <div class="text-wrapper">
                                    @Html.ActionLink("Exam", "Index", "Exam")
                                </div>
                            </div>
                        </div>
                    </li>
                    <li class="dropdown-divider"></li>
                                        <!---->

                    <li class="nav-item">
                        <div class="nav-link" style="margin-top: 10px">
                            <div class="user-wrapper">
                                <div class="profile-image">

                                </div>
                                <div class="text-wrapper">
                                    @Html.ActionLink("Schedules", "Index", "Schedules")
                                </div>
                            </div>
                        </div>
                    </li>
                    <li class="dropdown-divider"></li>

                </ul>
            </nav>

            <div class="main-panel">

                <div class="content-wrapper">
                    <div class="card">
                        <div class="card-body" style="min-height: 500px">
                            @RenderBody()
                        </div>
                    </div>
                </div>

                <footer class="footer">
                    <div class="container-fluid clearfix">
                        <span class="text-muted d-block text-center text-sm-left d-sm-inline-block">
                            Copyright © 2018
                            <a href="/" target="_blank">FPT Oil-Gas</a>. All rights reserved.
                        </span>
                    </div>
                </footer>
                <!-- partial -->
            </div>
            <!-- main-panel ends -->
        </div>
        <!-- page-body-wrapper ends -->
    </div>

    <script src="~/Assets/default/vendors/js/vendor.bundle.base.js"></script>
    <script src="~/Assets/default/vendors/js/vendor.bundle.addons.js"></script>
    <script src="~/Assets/default/js/off-canvas.js"></script>
    <script src="~/Assets/default/js/misc.js"></script>
    <script src="~/Assets/default/js/dashboard.js"></script>



    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
